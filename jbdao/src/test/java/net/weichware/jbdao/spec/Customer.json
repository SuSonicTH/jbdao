{
  "packagePath": "net.weichware.myapp",
  "name": "Customer",
  "with": true,
  "database": true,
  "json": true,
  "csv": true,
  "toString": true,
  "builder": true,
  "noArgsConstructor": true,
  "allArgsConstructor": true,

  "members": [
    {
      "name": "id",
      "type": "long",
      "primary": true,
      "csvName": "Index"
    },
    {
      "name": "firstName",
      "type": "String",
      "csvName": "First Name"
    },
    {
      "name": "lastName",
      "type": "String",
      "csvName": "Last Name"
    },
    {
      "name": "birthDate",
      "type": "LocalDate",
      "nullable": true,
      "maskPattern": "^(.+)..",
      "maskReplace": "$1xx",
      "csv": false
    },
    {
      "name": "address",
      "type": "String",
      "nullable": true,
      "default": "Unknown",
      "min": "3",
      "max": "50",
      "csv": false
    },
    {
      "name": "country",
      "type": "String",
      "nullable": true,
      "csv": false
    },
    {
      "name": "postalCode",
      "type": "Integer",
      "nullable": true,
      "default": 9999,
      "csv": false
    },
    {
      "name": "phoneNumber",
      "type": "String",
      "nullable": true,
      "pattern": "\\+[1-9][0-9]+",
      "maskPattern": "^(.+)....",
      "maskReplace": "$1xxxx",
      "csv": false
    },
    {
      "name": "kids",
      "type": "int",
      "nullable": true,
      "default": "0",
      "min": "0",
      "max": "10",
      "immutable": false,
      "toString": false,
      "csv": false
    }
  ]
}