{
  "packagePath": "net.weichware.jbdao",
  "name": "Customer",
  "with": true,
  "database": true,
  "json": true,
  "toString": true,
  "builder": true,
  "noArgsConstructor": true,
  "allArgsConstructor": true,
  "members": [
    {
      "name": "id",
      "type": "long",
      "primary": true
    },
    {
      "name": "firstName",
      "type": "String"
    },
    {
      "name": "lastName",
      "type": "String"
    },
    {
      "name": "birthDate",
      "type": "LocalDate",
      "nullable": true
    },
    {
      "name": "address",
      "type": "String",
      "nullable": true,
      "default": "Unknown",
      "min": "3",
      "max": "50"
    },
    {
      "name": "country",
      "type": "String",
      "nullable": true
    },
    {
      "name": "postalCode",
      "type": "Integer",
      "nullable": true,
      "default": 9999
    },
    {
      "name": "phoneNumber",
      "type": "String",
      "nullable": true,
      "pattern": "\\+[1-9][0-9]+"
    },
    {
      "name": "kids",
      "type": "int",
      "nullable": true,
      "default": "0",
      "min": "0",
      "max": "10",
      "immutable": false
    }
  ]
}